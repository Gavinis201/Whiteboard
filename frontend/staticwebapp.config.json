{
  "routes": [
    {
      "route": "/api/*",
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Credentials": "true"
      }
    }
  ],
  
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/static/*", "/assets/*", "/*.css", "/*.js", "/*.png", "/*.ico", "/*.jpg"]
  }
}